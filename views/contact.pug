doctype html
html
  title="Khan Coaching Academy"
  head
    style
      include ../static/styleContact.css
  body
    nav#navbar
      ul
        img(src="../static/logo.jpg", alt="image")
        li #[a(href="/home") Home]
        li #[a(href="/alumni") Alumni ]
        li #[a(href="/Scholarship") Scholarship ]
        li #[a(href="/Gallery") Gallery ]
        li #[a(href="/Centers") Centers ]
        li #[a(href="/About") About ]
        li #[a(href="/contact") Contact]
    h1 FEEL FREE TO CONTACT US 
    form(action="/contact", method="post" class="contact")       
      input(type="text" class="name" name="name" placeholder="Enter Your Name")
      input(type="text" class="name" name="parents" placeholder="Enter Your Parentage")
      input(type="text" class="name" name="residence" placeholder="Enter Your Residance")
      input(type="number" class="name" name="contact" placeholder="Enter Your Contact Number")
      input(type="text" class="name" nam="division" placeholder="Enter Your Standard")
      input(type="text" class="name" name="email" placeholder="Enter Your Email Address")
      button(type="submit" class="btn") Submit
    footer#footer 
      h2 © ℗ Khan Coaching Academy
      div.footer
        img(src="../static/facebook.jpeg")
        img(src="../static/twitter.jpeg")
        img(src="../static/linkedin.jpeg")
        img(src="../static/instagaram.jpeg")
        img(src="../static/email.jpeg")
    script 
      include ../static/index.js
    